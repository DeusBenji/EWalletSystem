{
    "ConnectionStrings": {
        "TokenServiceConnection": "Server=localhost;Database=TokenServiceDB;Trusted_Connection=True;TrustServerCertificate=True;Encrypt=False",
        "Redis": "localhost:6379"
    },
    "Fabric": {
        "BaseUrl": "http://localhost:8080"
    },
    "VcSigning": {
        "IssuerDid": "did:example:bachelor-token-service",
        "PrivateKeyPath": "keys/issuer-private.pem"
    },
    "Kafka": {
        "BootstrapServers": "localhost:9092",
        "Topics": {
            "TokenIssued": "token-issued",
            "MitIdVerified": "mitid-verified"
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "TokenService": "Debug"
        }
    },
    "AllowedHosts": "*"
}